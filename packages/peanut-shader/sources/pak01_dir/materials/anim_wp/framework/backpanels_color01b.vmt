LightMappedGeneric
{
$basetexture "anim_wp/framework/backpanels_color01b"
$bumpmap "Metal/metalwall_bts_006a_normal"
"%keywords" "portal2 bts destruction"
$surfaceprop metal
$envmap "metal/black_wall_envmap_002a"
hdr
{
$envmap "metal/black_wall_envmap_002a_hdr"
}
$envmapcontrast 1
$envmapsaturation .2
$envmaptint "[.25 .25 .25 ]"
$normalmapalphaenvmapmask 1
$envmaplightscale .6
"%noportal" 1
$reflectivity "[ .3 .3 .3 ]" 

                "$texscale"	4
	"$bumpscale"         4
	"$baseTextureOffset" "[0.5 0.5]"
	"$bumpTextureOffset" "[0.5 0.5]"
	"Proxies"
     	{
       "TextureTransform"
            {
		   			"translateVar" "$baseTextureOffset"
		   			"scaleVar"     "$texscale"
       			"resultVar"    "$baseTextureTransform"
            }
     	       "TextureTransform"
            {
		   			"translateVar" "$bumpTextureOffset"
		   			"scaleVar"     "$bumpscale"
       			"resultVar"    "$bumptransform"
            }
   
     	}

}
